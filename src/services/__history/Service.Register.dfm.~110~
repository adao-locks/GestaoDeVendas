    Connection = ServiceConnection.FDConn
    SQL.Strings = (
      'SELECT'
      #9'SL.ID_SALE,'
      #9'SL.ID_CLIENT,'
      #9'PO1.NAME NAME_CLI,'
      #9'SL.ID_EMPLOYEE,'
      #9'PO2.NAME NAME_EMPL,'
      #9'SL.PAYMENT_METHOD,'
      #9'SL.TOTAL_AMOUNT,'
      #9'SL.DISCOUNT,'
      #9'SL.STATUS,'
      #9'SL.DT_SALE,'
      #9'SL.DT_CREATED,'
      #9'SL.ID_COM,'
      #9'SL."USER",'
      #9'SL.OBSERVATION'
      'FROM'
      #9'SALE SL'
      'INNER JOIN PEOPLE PO1 ON'
      #9'SL.ID_CLIENT = PO1.PEOPLE_ID'
      'INNER JOIN PEOPLE PO2 ON'
      #9'SL.ID_EMPLOYEE = PO2.PEOPLE_ID'
      'WHERE'
      #9'1 = 1;')
